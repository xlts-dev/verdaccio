{
  "private": true,
  "name": "@verdaccio/website",
  "version": "5.2.0",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "netlify:build:production": "docusaurus write-translations && pnpm -w crowdin:sync && docusaurus build",
    "netlify:build:deployPreview": "docusaurus write-translations --locale en && docusaurus build --locale en",
    "eslint:check": "eslint src/**/*.tsx",
    "eslint:write": "eslint src/**/*.tsx --fix",
    "prettier:check": "prettier src/**/* --check",
    "prettier:write": "prettier src/**/* --write --config ../prettier.config.js"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.9",
    "@docusaurus/plugin-google-analytics": "2.0.0-beta.9",
    "@docusaurus/preset-classic": "2.0.0-beta.9",
    "@docusaurus/remark-plugin-npm2yarn": "2.0.0-beta.9",
    "@docusaurus/theme-search-algolia": "2.0.0-beta.9",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "copy-text-to-clipboard": "^3.0.1",
    "docusaurus-plugin-sentry": "^1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-twitter-widgets": "^1.10.0",
    "use-is-in-viewport": "^1.0.9"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.9",
    "@tsconfig/docusaurus": "^1.0.2",
    "docusaurus-plugin-sass": "^0.2.1",
    "esbuild": "0.13.13",
    "esbuild-loader": "2.16.0",
    "prism-react-renderer": "^1.2.1",
    "sass": "1.39.0",
    "sass-loader": "^12.1.0",
    "url-loader": "4.1.1"
  }
}
